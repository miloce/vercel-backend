{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ],
  "env": {
    "WECHAT_APP_ID": "@wechat_app_id",
    "WECHAT_APP_SECRET": "@wechat_app_secret"
  }
} 
